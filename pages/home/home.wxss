/* theme.wxss */

page {
  background-color: #f0f2f5;
}

view,
image {
  box-sizing: border-box;
  line-height: 1;
}

.choose-card .card.select {
  background-color: #fff;
  border: 3rpx solid #1aad19;
  color: #1aad19;
}

.choose-card .card-txt {
  line-height: 130rpx;
  background-color: #f0f2f5;
  border-radius: 10rpx;
  text-align: center;
  font-size: 32rpx;
}

.ecard-money {
  position: relative;
  padding: 0 30rpx;
  height: 106rpx;
  line-height: 106rpx;
  background-color: #fff;
  font-size: 32rpx;
}

.ecard-money-input {
  float: right;
  line-height: inherit;
}

.ecard-money-input input {
  float: left;
  margin: 28rpx 12rpx 0 0;
  width: 160rpx;
  text-align: right;
}

.banner {
  padding: 40rpx 0;
  background-color: #fff;
  text-align: center;
}

.banner image {
  width: 487rpx;
  height: 292rpx;
  border-radius: 8px;
}

.card-choose {
  padding-top: 6rpx;
  background-color: #fff;
}

.choose-title {
  padding-left: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-left: 3px solid #f23030;
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
}

.card-choose-wrap {
  display: flex;
  overflow-x: scroll;
  margin-right: 30rpx;
  padding: 16rpx 0 20rpx 30rpx;
  -webkit-overflow-scrolling: touch;
}

.choose-pic {
  position: relative;
  flex: none;
  margin-right: 16rpx;
  width: 200rpx;
  height: 120rpx;
}

.card-mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 200rpx;
  height: 120rpx;
  background-color: rgba(255, 255, 255, 0.4);
  border-radius: 8px;
}

.choose-pic image {
  position: relative;
  width: 200rpx;
  height: 120rpx;
  border-radius: 8px;
}

.card-cur {
  border: 2px solid #f23030;
}

.choose-pic:last-child {
  margin-right: 0;
}

.card-cur .card-selected {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 45rpx;
  height: 42rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}

.value-choose {
  padding-top: 30rpx;
  background-color: #fff;
}

.choose-title-lf {
  float: left;
  height: 60rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  line-height: 60rpx;
}

.choose-title-rt {
  float: right;
  height: 60rpx;
  line-height: 60rpx;
  padding-right: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.choose-title-rt-img {
  margin-right: 10rpx;
  width: 30rpx;
  height: 30rpx;
}

.choose-title-rt-cont {
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #f23030;
}

.value-choose-wrap {
  padding: 20rpx 4% 15rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 100%;
  justify-content: space-between;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.value-item {
  margin-bottom: 20rpx;
  width: 335rpx;
  height: 188rpx;
  box-shadow: 0px 5px 21px 3px rgba(74, 76, 91, 0.05);
  border-radius: 10rpx;
  border: solid 1px #eeeeee;
}

.choice-item {
  border: solid 1px #f23030 !important;
}

.value-item-top {
  width: 100%;
  height: 110rpx;
  padding: 20rpx 30rpx 0 30rpx;
}

.value-l-hd {
  font-family: PingFangSC-Light;
  height: 30rpx;
  font-size: 24rpx;
  line-height: 30rpx;
  color: #666666;
}

.value-l-bd {
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  font-weight: 20rpx 30rpx 0 30rpx;
  line-height: 60rpx;
  color: #232326;
  display: flex;
  align-items: center;
  height: 60rpx;
}

.value-l-edit {
  width: 60rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 50%;
}

.value-l-bd-input {
  width: 100%;
  height: 100%;
}

.value-item-bottom {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 78rpx;
  line-height: 78rpx;
  justify-content: space-around;
  align-items: center;
  box-sizing: border-box;
  border-top: 1px solid #eee;
}

.value-input {
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
  padding-left: 30rpx;
  height: 120rpx;
  border: 1px solid #ccc;
  border-radius: 8px;
}

.value-input input {
  width: 100%;
  height: 32rpx;
  font-size: 28rpx;
}

.icon-edit {
  display: inline-block;
  margin: -20rpx 0 -20rpx 10rpx;
  width: 86rpx;
  height: 66rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 26rpx 26rpx;
  background-position: center;
}

.value-oprt-add {
  width: 25rpx;
  height: 25rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -12.5rpx;
  margin-left: -12.5rpx;
}

.value-oprt-minus {
  width: 25rpx;
  height: 25rpx;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABkAAAAECAYAAAB7oZQmAAAABGdBTUEAALGPC/xhBQAAACBjSFJNAAB6JgAAgIQAAPoAAACA6AAAdTAAAOpgAAA6mAAAF3CculE8AAAABmJLR0QAAAAAAAD5Q7t/AAAACXBIWXMAAAsSAAALEgHS3X78AAAAKUlEQVQY07XNAQEAIAgDsEku8lFM7GWKr8DOzFy0nK1wAF3hABQ2fLwPtmQFXgZ56ZkAAAAASUVORK5CYII=) no-repeat center;
  background-size: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -12.5rpx;
  margin-left: -12.5rpx;
}

.value-num {
  margin: 0 -32rpx;
  text-align: center;
  font-size: 26rpx;
  width: 56rpx;
  height: 56rpx;
  background-color: #fff6f6;
  border-radius: 50%;
  line-height: 56rpx;
  color: #f23030;
}

.luck-input-wrap {
  margin-bottom: 20rpx;
  padding: 30rpx 4% 30rpx;
}

.luck-input {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
  padding: 0 40rpx 0 30rpx;
  height: 120rpx;
  border: 1rpx solid #ccc;
  border-radius: 16rpx;
  font-size: 26rpx;
  font-family: 'PingFangSC-Medium';
}

.luck-input-bd {
  display: flex;
  align-items: center;
}

.luck-input-bd input {
  margin-right: 20rpx;
  width: 400rpx;
  height: 30rpx;
  text-align: right;
}

.luck-unit {
  font-weight: 700;
}

.luck-tip {
  font-size: 24rpx;
  color: #aaa;
}

.empty {
  text-align: center;
}

.empty .icon-empty1 {
  margin: 136rpx 0 100rpx;
  width: 233rpx;
  height: 214rpx;
  background-image: url(data:image/png;base64,);
}

.empty .icon-empty2 {
  margin: 176rpx 0 90rpx;
  width: 233rpx;
  height: 175rpx;
  background-image: url(data:image/png;base64,);
}

.empty .empty-txt {
  font-size: 30rpx;
  color: #888893;
}

.info-btn {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  height: 106rpx;
  background-color: #fff;
  border-top: 1rpx solid #eee;
}

.btn-info {
  flex: 1;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  border-top: 1rpx solid #e3e5e9;
}

.btn-info-hd {
  margin-bottom: 10rpx;
  font-size: 24rpx;
  color: #999;
  margin-left: -8rpx;
}

.btn-info-bd {
  font-size: 28rpx;
}

.btn-info-bd .emphaz {
  display: inline-block;
  font-size: 36rpx;
}

.btn-box {
  width: 250rpx;
  line-height: 106rpx;
  text-align: center;
  background-color: #bfbfbf;
  font-size: 36rpx;
  color: #fff;
  font-family: 'PingFangSC-Medium';
  border-radius: 0;
  padding: 0;
  margin: 0;
}

.btn-box::after {
  border: 0;
}

.btn-box-active {
  background-color: #f23030;
}

.trans-left {
  transform: translateX(-100%);
  transition: all ease .5s;
}

.trans-right {
  transform: translateX(100%);
  transition: all ease .5s;
}

.value-oprt-wrap {
  width: 50rpx;
  height: 50rpx;
  position: relative;
  background-color: #FFF;
  padding: 0;
}

.value-oprt-wrap::after {
  padding: 0;
  margin: 0;
  border: 0;
}