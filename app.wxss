/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.container.wrap {
  /*margin-bottom: 118rpx;*/
  padding-bottom: 130rpx;
}

.mb20{
    margin-bottom: 20rpx;
}

.icon {
    display: inline-block;
    vertical-align: middle;
    background-size: cover;
    background-repeat: no-repeat;
}

.icon.icon-item {
    margin-right: 10rpx;
    margin-bottom: 6rpx;
}

.icon.icon-warn, .icon.icon-okay {
    width: 52rpx;
    height: 52rpx;
    background-image: url(data:image/png;base64,);
}

.icon.icon-okay {
    background-image: url(data:image/png;base64,);
}

.icon-loading {
    margin-bottom: 20rpx;
    width: 52rpx;
    height: 52rpx;
    background-image: url(http://www.wanyus.com/uba/jfs/t19033/93/943707955/23978/ce99c6cf/5ab35bd8N1d7801ac.gif)
}

.btn {
    text-align: center;
    background-color: #f23030;
    border-radius: 3px;
    color: #fff;
}

.btn.disabled {
    background-color: #ccc;
}

.btn.btn-red {
    background-color: #f23030;
     border-radius: 10rpx;
}

.bind {
    /*margin-top: 2.5rpx;*/
    /*position: absolute;*/
    /*bottom: 119rpx;*/
    /*left: 0;*/
    /*width: 100%;*/
    padding: 40rpx 0;
    text-align: center;
    font-size: 26rpx;
    color: rgb(48, 150, 255);
}

.bind-bottom {
    position: absolute;
    width: 100%;
    bottom: 90rpx;
}

.link {
    display: inline-block;
    font-size: 26rpx;
    color: #3096ff;
    /*color: #4a6da2;*/
}

.btn-fixed {
    position: relative;
    position: fixed;
    bottom: 0;
    padding: 20rpx;
    width: 100%;
    /*height: 128rpx;*/
    background-color: #fff;
    border-top: 1rpx solid #e3e5e9;
    /*box-shadow: 0px -2px 3px rgba(0, 0, 0, 0.04);*/
    /*border-bottom: 1rpx solid #f0f2f5;*/
    z-index: 9;
}

.fix-ipx-btn-fixed {
    bottom: 30rpx!important;
}

.fix-ipx-btn-fixed::after {
    content: '';
    position: fixed;
    left: 0!important;
    bottom: 0rpx!important;
    height: 30rpx!important;
    width: 100%;
    background: #FFF;
}

.btn-fixed .btn {
    height: 78rpx;
    line-height: 78rpx;
    font-size: 28rpx;
    letter-spacing: 1rpx;
}

.btn-history {
    margin: 120rpx auto 0;
    width: 450rpx;
    height: 78rpx;
    line-height: 78rpx;
    font-size: 28rpx;
}

.btn-detail, .btn-bind, .btn-white {
    margin: 0 auto;
    width: 451rpx;
    height: 78rpx;
    line-height: 78rpx;
    font-size: 28rpx;
}

.btn-detail {
    margin-top: 54rpx;
}

.btn-detail-gray {
    background-color: #bfbfbf;
}

.btn-white {
    margin-top: 40rpx;
    background-color: #fff;
    border: 1rpx solid #f23030;
    color: #f23030;
}

.other-message {
    margin-bottom: 23rpx;
    padding: 0 30rpx;
    background-color: #fff;
}

.other-message.has-margin {
    margin-top: 20rpx;
    padding-bottom: 120rpx;
}

.other-message .message-item {
    overflow: hidden;
    position: relative;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #f0f2f5;
    font-size: 26rpx;
}

.message-item:last-child {
    border-bottom: none;
}

.msg-hd {
    float: left;
    width: 150rpx;
    line-height: 1.2;
    color: #666;
}

.msg-bd {
    display: inline-block;
    color: #333;
    font-weight: 600;
    float: left;
    width: 508rpx;
    line-height: 1.2;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.message-item.li-received {
    height: auto;
}

.msg-bd-received {
    width: 455rpx;
    /*padding-bottom: 4rpx;*/
    display: inline-block;
    line-height: 1;
    vertical-align: text-top;
}

.received-txt {
    margin-bottom: 18rpx;
}

.received-txt:last-child {
    margin-bottom: 0;
}

.tip-warn {
    margin-right: 8rpx;
    width: 24rpx;
    height: 24rpx;
}

.other-message .message-item .msg-avatar {
    position: absolute;
    top: 34rpx;
    right: 30rpx;
    width: 64rpx;
    height: 64rpx;
}

.card-list-item {
    /*position: relative;*/
    display: flex;
    margin-bottom: 10rpx;
    padding: 40rpx 36rpx 40rpx 33rpx;
    overflow: hidden;
    /*border-bottom: 1px solid #f0f2f5;*/
    background-color: #fff;
    border-radius: 4px;
    box-shadow: 0rpx 3rpx 20rpx rgba(0, 0, 0, 0.1)
}

.card-list-item:last-child {
    margin-bottom: 0;
}

.history-item-hd, .history-item-hd2{
    position: relative;
    margin-right: 30rpx;
    width: 172rpx;
    height: 106rpx;
}

.history-item-hd image {
    position: relative;
    width: 172rpx;
    height: 106rpx;
    border: 1rpx solid #e3e5e9;
    border-radius: 4px;
    z-index: 2;
}

.cards-border {
    position: absolute;
    width: 100%;
    height: 100%;
    border: 1rpx solid #e3e5e9;
    border-radius: 4px;
    background-color: #fff;
}

.cards-border1 {
    top: 5rpx;
    left: 5rpx;
    z-index: 1;
}

.cards-border2 {
    top: 10rpx;
    left: 10rpx;
}

.cards-tag, .cards-tag2 {
    position: absolute;
    top: 0;
    left: 0;
    padding: 0 4rpx;
    height: 24rpx;
    background: rgba(0, 0, 0, .7);
    line-height: 24rpx;
    text-align: center;
    border-top-left-radius: 4px;
    border-bottom-right-radius: 4px;
    font-size: 20rpx;
    color: #fff;
    z-index: 3;
}

.history-item-bd {
    flex: 1;
}

.history-item-hd2{
    width: 180rpx;
    height: 180rpx;
}

.history-item-hd2 image {
    position: relative;
    width: 180rpx;
    height: 180rpx;
    border: 1rpx solid #e3e5e9;
    /*border-radius: 4px;*/
    z-index: 2;
}

.cards-tag2{
    border-top-left-radius: 0;
}

.card-name {
    line-height: 1.2;
    font-size: 30rpx;
    font-family: 'PingFangSC-Medium';
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-bottom: 30rpx;
}

.card-price {
    margin-top: 30rpx;
    font-size: 36rpx;
}

.card-price-sign {
    display: inline-block;
    font-size: 24rpx;
    font-weight: 700;
}

.pt100 {
    padding-top: 100rpx;
}

.history-empty {
    padding-top: 400rpx;
    text-align: center;
    background: #f1f1f1 url(data:image/png;base64,) no-repeat center 188rpx/257rpx 169rpx;
    font-size: 28rpx;
    color: #666;
}

.notice{
  width: 750rpx;
  height: 72rpx;
  background-color: #fdfae6;
  color: #eb7218;
  font-size: 24rpx;

  display: flex;
  justify-content: flex-start;
  align-items: center;

  position: fixed;
  left: 0;
  top: 0;
}

.notice .img{
  width: 28rpx;
  height: 34rpx;
  margin-right: 15rpx;
  margin-left: 28rpx;
  background: url(https://www.wanyus.com/uba/jfs/t20662/29/2276596986/938/7836381b/5b4d6358N837fbb06.png) no-repeat;
  background-size: 28rpx 34rpx;
}

.notice .tip{
  width: 580rpx;
}

.bottom-btn-box{
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;

  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #FFF;
  border-top: 1rpx solid #e3e5e9;
}

.bottom-btn-box .btn{
  width: 711rpx;
  height: 78rpx;
  border-radius: 4rpx;
  background-color: #f23030;
  color: #FFF;
  font-size: 24rpx;
  text-align: center;
  line-height: 78rpx;
}

.bottom-btn-box .btn-disabled{
  width: 711rpx;
  height: 78rpx;
  border-radius: 4rpx;
  background-color: #bfbfbf;
  color: #FFF;
  font-size: 24rpx;
  text-align: center;
  line-height: 78rpx;
}

.blist-hd-check {
  width: 36rpx;
  height: 36rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  padding: 0;
  margin: 0 10rpx 0 0;
  border: 0;
}

.blist-hd-check:after {
  content: '';
  width: 0;
  height: 0;
  display: none;
  background-color: transparent;
  transform: scale(1);
}

.blist-hd-check.not-checked {
  width: 32rpx;
  height: 32rpx;
  background: none;
  border: 1px solid #999aa3;
  border-radius: 50%;
}

.blist-hd-check.check-disabled{
  border: 1rpx solid #efefef;
  background-image: none;
  background-color: #f2f2f2;
  border-radius: 50%;
}


.shade-bloading{
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  padding: 0 100rpx;
  /*background: rgba(0, 0, 0, 0.2);*/
  text-align: center;
  z-index: 10;
}

.bloading{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
  width: 200rpx;
  height: 160rpx;
  background-color: rgba(0, 0, 0, 0.6);
  border-radius: 16rpx;
}

.bloading i{
  display: block;
  margin-bottom: 10rpx;
  width: 60rpx;
  height: 60rpx;
  background-image: url(http://www.wanyus.com/uba/jfs/t1/1298/22/148/948/5b8fa2e8E7f6fffb1/7d3e5829ed0b495f.png);
  background-size: 100% 100%;
  animation: loading 1s 0s linear infinite;
}

@keyframes loading {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

